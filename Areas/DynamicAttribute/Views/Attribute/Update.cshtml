@model AttributeVM

<h1>Update Category</h1>

<form method="POST" asp-area="DynamicAttribute" asp-controller="Attribute" asp-action="Update">
    <div asp-validation-summary="All" class="text-danger"></div>

    <div>
        <label asp-for="Name"></label>
        <input asp-for="Name">
        <span asp-validation-for="Name"></span>
    </div>


    <button type="submit">Cập nhật thuộc tính động</button>
</form>